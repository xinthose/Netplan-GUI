{
  "apps": [
    {
      "name": "netplan-gui",
      "script": "./server.js",
      "watch": false,
      "max_memory_restart": "256M",
      "output": "/var/www/html/logs/netplan-gui-out.log",
      "error": "/var/www/html/logs/netplan-gui-error.log",
      "kill_timeout": 5000,
      "restartDelay": 5000
    },
    {
      "name": "netplan-gui-rest",
      "script": "gunicorn -w 4 -b 0.0.0.0:8080 -k uvicorn.workers.UvicornWorker rest:app -chdir /var/www/html/netplan-gui/assets/rest",
      "max_memory_restart": "256M",
      "output": "/var/www/html/logs/netplan-gui-rest-out.log",
      "error": "/var/www/html/logs/netplan-gui-rest-error.log",
      "kill_timeout": 5000,
      "restartDelay": 5000
    }
  ]
}
